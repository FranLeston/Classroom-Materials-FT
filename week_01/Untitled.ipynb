    "import datetime\n",
    "now = datetime.datetime.now()\n",
    "\n",
    "try:\n",
    "    age = int(input(\"¿Que edad tienes? \"))\n",
    "    print(\"You had a birthday in each of these years:\")\n",
    "    print([now.year-year for year in range(age)])\n",
    "except:\n",
    "    print(\"Oops, please enter a valid age!\")"
